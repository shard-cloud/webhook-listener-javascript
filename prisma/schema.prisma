generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE")
}

model WebhookEvent {
  id        String   @id @default(cuid())
  source    String
  eventType String
  payload   Json
  headers   Json
  ip        String?
  userAgent String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@map("webhook_events")
}
